{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "target": "es6",
    "lib": ["es2017", "dom"],
    "moduleResolution": "node",
    "moduleSuffixes": [".ios", ".native", ""]
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}
